{
  "name": "medusa-furgonetka-elements",
  "version": "0.1.0",
  "description": "Plugin for using Furgonetka API to create shipping labels directly from admin dashboard",
  "main": "dist/index.js",
  "author": "Falko Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/falko-project/medusa-furgonetka-elements"
  },
  "keywords": [
    "medusa-plugin",
    "medusa-plugin-shipping",
    "medusa-furgonetka-elements",
    "medusa-plugin-admin",
    "furgonetka",
    "medusajs"
  ],
  "scripts": {
    "clean": "cross-env ./node_modules/.bin/rimraf dist",
    "build": "cross-env npm run clean && npm run build:server && npm run build:admin",
    "build:server": "cross-env npm run clean && tsc -p tsconfig.json",
    "build:admin": "cross-env medusa-admin build",
    "prepare": "cross-env NODE_ENV=production npm run build:server && medusa-admin bundle"
  },
  "dependencies": {
    "@medusajs/admin": "7.1.16",
    "@medusajs/ui": "^3.0.0",
    "@medusajs/utils": "1.11.10", 
    "@medusajs/icons": "1.2.2",
    "@tanstack/react-query": "4.22",
    "medusa-interfaces": "^1.3.8",
    "medusa-react": "9.0.18",
    "@mui/material": "^6.1.3",
    "typeorm": "^0.3.16",
    "react-hook-form": "^7.49.2",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@medusajs/medusa": "^1.20.10",
    "@types/express": "^4.17.13",
    "babel-preset-medusa-package": "^1.1.19",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "@medusajs/medusa": "^1.20.10",
    "react": "^18.2.0",
    "react-router-dom": "^6.13.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.spec.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/node_modules/"
    ],
    "rootDir": "src",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "transform": {
      ".ts": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
